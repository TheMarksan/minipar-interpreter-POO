C_CHANNEL canal;

VOID produtor() {
    INT i;
    for i = 1; i <= 3; i = i + 1 {
        print("Produtor enviando:");
        print(i);
        canal.send(i);
    }
}

VOID consumidor() {
    INT valor;
    INT i;
    for i = 1; i <= 3; i = i + 1 {
        canal.receive(valor);
        print("Consumidor recebeu:");
        print(valor);
    }
}

PAR {
    produtor();
    consumidor();
}

SEQ {
    print("Execucao cliente-servidor finalizada");
}
